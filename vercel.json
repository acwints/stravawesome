{
  "version": 2,
  "buildCommand": "npx prisma generate && SKIP_TYPESCRIPT=1 next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sfo1"],
  "github": {
    "enabled": true,
    "silent": true,
    "autoAlias": true
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "development": false,
      "preview": false
    }
  }
} 